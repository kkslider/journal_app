<h1><%= title %></h1>

<ul>
  <% posts.forEach(function (post) { %>
    <li class="post">
      <a href="#/posts/<%= post.id %>"><%= post.escape("title") %></a>&nbsp;
      <button type="button" class="delete" data-id="<%= post.id %>">Delete!!</button>
    </li>
  <% }); %>
</ul>

<a href="#/posts/new">Create new post</a>
